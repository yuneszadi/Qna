<% if  @question.errors.present? %>
  <% @question.errors.full_messages.each do |message| %>
    $('.question_errors').append('<%= j message %><br>')
  <% end %>
<% else %>
  $('.question_errors').html('')
  $('.edit-question-link').click(function (e) {
    console.log('event')
    e.preventDefault();
    $('.edit_question').show();
    $(this).hide();
  })
  location.reload()
<% end %>
